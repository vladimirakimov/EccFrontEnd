{
    "presets": [
        "env",
        "react",
        "es2015",
        "react-app"
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs"
            ]
        }
    },
    "plugins": [
        ["babel-plugin-root-import", {
            "rootPathSuffix": "src"
        }]
    ],
    "ignore": [
        "node_modules"
    ]
}